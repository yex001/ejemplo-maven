pipeline {
  agent any
  stages {
    stage("Check Environment Variables") {
      steps {
        script {
          sh 'printenv'
        }
      }
    }
    stage("build & sonarqube") {
      steps {
        script {
          withSonarQubeEnv('SonarQube_server') {
            sh 'mvn clean package sonar:sonar'
          }
        }
      }
    }
  }
}
